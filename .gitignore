# Include resource files
!.eslintrc.js
!**/**/.lintstagedrc.js
!**/**/jest.config.js

# Node artifact files
node_modules/
dist/
build/
**/node_modules
**/dist
**/build
**/lib
# **/*.d.ts
# **/*.js

# environment files
.env.*.local
.env.local

# prevent npm lock
package-lock.json


# Compiled Java class files
*.class

# Compiled Python bytecode
*.py[cod]

# Log files
*.log

# Package files
*.jar

# Maven
target/

# JetBrains IDE
.idea/

# Unit test reports
**/*.xml

# Testing artifacts
coverage
**/coverage

# Generated by MacOS
.DS_Store

.yarn/*
!.yarn/releases
!.yarn/patches
!.yarn/plugins
!.yarn/sdks
!.yarn/versions
.pnp.*

# CDK build
stack/**/*.js
stack/**/*.d.ts

# CDK asset staging directory
**/.cdk.staging
**/cdk.out

# Generated by Windows
Thumbs.db

# Applications
*.app
*.exe
*.war

# Large media files
*.mp4
*.tiff
*.avi
*.flv
*.mov
*.wmv
mongo-volume
.vscode
.idea

#Localstack files
.localstack
# Elastic Beanstalk Files
.elasticbeanstalk/*
!.elasticbeanstalk/*.cfg.yml
!.elasticbeanstalk/*.global.yml
